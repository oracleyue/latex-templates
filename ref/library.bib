@phdthesis{Balakrishnan1995,
abstract = {Control system design has traditionally been based on a model of the system to be controlled. When the system operates in a stationary or slowly varying environment, controllers designed on the basis of a single fixed or adaptive model perform satisfactorily. As systems become more and more complex, however, a new class of control problems are encountered in which the operating environment changes frequently and abruptly between different contexts. Sudden changes in subsystem dynamics, disturbances which occur in bursts, failures in sensors or actuators, and changes in reference trajectories to be tracked, are examples of different environments in which complex systems must operate. The task of a control system in such problems is to quickly recognize the environment which exists, and rapidly provide the appropriate control action. Conventional robust or adaptive control systems based on a single model cannot provide such a function, and more "intelligent" control methods are required. The thesis proposes a general methodology based on multiple models, switching, and tuning, for dealing with such control problems. Multiple models are used to describe the different environments approximately. Switching is used to select the best model rapidly and tuning is used to further improve its accuracy. Models for newly encountered environments can be learnt as the system is in operation. A theoretical basis for the proposed methodology is provided in the context of model reference control of time-invariant and rapidly switching systems. The methods were applied to simulation examples of control in the presence of changes in subsystem dynamics, aircraft control in the presence of actuator failure, and camera pan angle tracking of maneuverable point targets. These and numerous other simulations presented in the thesis demonstrate that the performance of the proposed methods is far superior to those based on a single model.},
address = {United States -- Connecticut},
annote = {Source type: dissertations{\&}theses; Object type: Dissertation; Object type: Thesis; Copyright: Copyright UMI - Dissertations Publishing 1995; DOCID: 739246171; PCID: 30115081; PMID: 66569; DocISBN: 9780591018929; DocISBN: 0591018926; DissertationNum: 9636081; PublisherXID: 9636081},
author = {Balakrishnan, Jeyendran},
booktitle = {ProQuest Dissertations and Theses},
editor = {Narendra, Kumpati S},
isbn = {9780591018929 0591018926},
keywords = {0544:Electrical engineering,0790:Systems design,Applied sciences,Electrical engineering,Systems design,adaptive control},
publisher = {Yale University},
title = {{Control system design using multiple models, switching, and tuning}},
url = {http://lbsearch.ust.hk:3210/sfx?url{\_}ver=Z39.88-2004{\&}rft{\_}val{\_}fmt=info:ofi/fmt:kev:mtx:dissertation{\&}genre=dissertations+{\%}26+theses{\&}sid=ProQ:ProQuest+Dissertations+{\%}26+Theses+A{\%}26I{\&}atitle={\&}title=Control+system+design+using+multiple+models{\%}2C+switching{\%}2C+and+tuning{\&}issn={\&}date=1995-01-01{\&}volume={\&}issue={\&}spage={\&}au=Balakrishnan{\%}2C+Jeyendran{\&}isbn=9780591018929{\&}jtitle={\&}btitle={\&}rft{\_}id=info:eric/ http://search.proquest.com/docview/304264227?accountid=29018},
