issn = {0962-8436; 0962-8436},
journal = {Philosophical transactions of the Royal Society of London.Series B, Biological sciences},
keywords = {Brain Mapping/methods,Brain/anatomy {\&} histology/physiology,Computer Simulation,Electroencephalography/methods,Humans,Magnetic Resonance Imaging/methods,Markov Chains,Models,Neurological,Oxygen/blood},
month = {may},
