isbn = {VO -},
keywords = {Approximation methods,Control systems,Convergence,Mathematical model,Noise,Noise measurement,Stochastic processes,approximation theory,computer aided control system design software pack,continuous time deterministic dynamical system,continuous time systems,control system CAD,discrete time statistically equivalent system,discrete time systems,linear time invariant CT SDS,noise statistic matrices,noisy measurements,numerical approximations,numerical simulation,software packages,state-space representation,stochastic dynamical system,stochastic systems},
pages = {1397--1402},
