@article{Ramb2013a,
abstract = {In the analysis of neuroscience data, the identification of task-related causal relationships between various areas of the brain gives insights about the network of physiological pathways that are active during the task. One increasingly used approach to identify causal connectivity uses the concept of Granger causality that exploits predictability of activity in one region by past activity in other regions of the brain. Owing to the complexity of the data, selecting components for the analysis of causality as a preprocessing step has to be performed. This includes predetermined-and often arbitrary-exclusion of information. Therefore, the system is confounded by latent sources. In this paper, the effect of latent confounders is demonstrated, and paths of influence among three components are studied. While methods for analysing Granger causality are commonly based on linear vector autoregressive models, the effects of latent confounders are expected to be present also in nonlinear systems. Therefore, all analyses are also performed for a simulated nonlinear system and discussed with regard to applications in neuroscience.},
author = {Ramb, Rebecca and Eichler, Michael and Ing, Alex and Thiel, Marco and Weiller, Cornelius and Grebogi, Celso and Schwarzbauer, Christian and Timmer, Jens and Schelter, Bj{\"{o}}rn},
doi = {10.1098/rsta.2011.0612},
file = {:Users/oracleyue/Papers/Mendeley/Ramb et al/Ramb et al. - 2013 - The impact of latent confounders in directed network analysis in neuroscience.pdf:pdf},
issn = {1364-503X},
journal = {Philosophical transactions. Series A, Mathematical, physical, and engineering sciences},
keywords = {Animals,Brain,Brain Mapping,Brain Mapping: methods,Brain: physiology,Computer Simulation,Factor Analysis,Humans,Models,Multivariate Analysis,Nerve Net,Nerve Net: physiology,Neurological,Neurosciences,Neurosciences: methods,Regression Analysis,Statistical,Synaptic Transmission,Synaptic Transmission: physiology},
pages = {20110612},
pmid = {23858480},
title = {{The impact of latent confounders in directed network analysis in neuroscience.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/23858480},
volume = {371},
year = {2013}
